{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "app_id": {
      "type": "string",
      "title": "应用编号"
    },
    "app_title": {
      "type": "string",
      "title": "应用标题"
    },
    "app_pages": {
      "type": "object",
      "title": "应用页面",
      "properties": {
        "ids": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "entities": {
          "type": "object"
        }
      },
      "required": [
        "ids", "entities"
      ]
    }
  }
}
