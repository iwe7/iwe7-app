{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "page_id": {
      "type": "string",
      "title": "页面编号"
    },
    "page_title": {
      "type": "string",
      "title": "页面标题"
    },
    "page_components": {
      "type": "object",
      "title": "页面内容",
      "properties": {
        "ids": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "entities": {
          "type": "object"
        }
      },
      "required": [
        "ids", "entities"
      ]
    }
  }
}
